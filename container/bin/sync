#! /bin/sh 

set -e

rclone sync dropbox:csdha_assets/public/build public/build
rclone sync dropbox:csdha_assets/public/font public/font
rclone sync dropbox:csdha_assets/public/icon public/icon
rclone sync dropbox:csdha_assets/public/images public/images
git pull
php artisan optimize:clear
php artisan migrate --force
php artisan db:audit-triggers
php artisan optimize
chown -R csdha:csdha bootstrap/cache storage

