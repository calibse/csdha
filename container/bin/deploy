#! /bin/sh

set -e

php artisan optimize:clear 
php artisan migrate --force
php artisan db:audit-triggers
php artisan optimize 
chown -R csdha:csdha bootstrap/cache storage 
cp -a storage/. /root/user_storage
cp -a storage/. /root/admin_storage
cp -a bootstrap/cache/. /root/user_cache
cp -a bootstrap/cache/. /root/admin_cache

